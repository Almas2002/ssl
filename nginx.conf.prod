server {
    listen 80;
    listen [::]:80;

    server_name platinum-autorent.com www.platinum-autorent.com;
    server_tokens off;

    location ~ /.well-known/acme-challenge {
        allow all;
        root /var/www/html;
    }

    location / {
        return 301 https://example.org$request_uri;
    }
}

